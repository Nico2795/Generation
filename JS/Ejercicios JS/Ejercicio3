function lowestNumber(numero){
    let min = numero[0];
    for(i=0; i<numero.length; i++){
        if(min>numero[i] && numero[i] < 0){
            min= numero[i]
            return "Hay un numero negativo en la posicion" + numero.indexOf(min)
        }
        else{ return "correcto"}

    }
    return min
}
console.log(lowestNumber([6,5,10,20,-100]))
